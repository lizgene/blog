         <div class="row rsvp-buttons mtxl">
          <div class="col-xs-6 col-sm-3 col-sm-offset-3">
            <a href="#yes" aria-controls="yes" role="tab" data-toggle="tab">YES</a>
          </div>
          <div class="col-xs-6 col-sm-3">
            <a href="#no" aria-controls="no" role="tab" data-toggle="tab">NO</a>
          </div>
        </div>

        <div class="row mvxl">

           <div class="tab-content">
            <div role="tabpanel" class="tab-pane" id="yes">

              <div class="row">
                <div class="col-sm-4 col-sm-offset-4">
                  <%= form_tag do %>
                    <h2 class="cursive">Get ready for adventure!</h2>

                    <div class="form-group">
                      <%= text_field_tag :name, "", placeholder: "Your Name", class: "form-control" %>
