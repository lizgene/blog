@font-face {
  font-family:'Grand Hotel';
  src: url(font-path('grandhotel-regular-webfont.eot'));
    src: url(font-path('grandhotel-regular-webfont.eot')) format('embedded-opentype'),
    url(font-path('grandhotel-regular-webfont.svg')) format('svg'),
    url(font-path('grandhotel-regular-webfont.ttf')) format('truetype'),
    url(font-path('grandhotel-regular-webfont.woff')) format('woff');
  font-weight: normal;
  font-style: normal;
} 

